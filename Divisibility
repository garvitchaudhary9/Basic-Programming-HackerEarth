#include <iostream>

using namespace std;

int main() {

    int N = 0;
    cin>>N;
    
    long data[N];
    for(int i=0; i<N; i++){
        cin>>data[i];
		
		
	}
    
    // write your code here
    // ans = 
	string ans;
	
    if(data[N-1]%10==0){
		ans="Yes";
	}
	
	else{
		ans="No";
	}
    
    cout<<ans;
    
    return 0;
}
